<h1>Login</h1>
  </head>
  <body>
    <%= form_for users_login_path do |f| %>
      <ul>
         <li>
            <%= f.label :username %>
            <%= f.text_field :username, autocomplete: :off %>
         </li>
         <li>
           <%= f.label :password %>
           <%= f.password_field :password %>
         </li>
      </ul>
      <%= f.submit "Login" %>
    <% end %>
