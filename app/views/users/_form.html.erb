<%= form_for users_path do |f| %>
<h1><%=@page_title%></h1>
  <ul>
     <li>
        <%= f.label :username %>
        <%= f.text_field :username, autocomplete: :off %>
     </li>
     <li>
       <%= f.label :password %>
       <%= f.password_field :password %>
     </li>
  </ul>
  <%= f.submit @submit_text%>
<% end %>
