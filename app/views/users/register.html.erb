    <h1>Register</h1>
  </head>
  <body>
    <%= form_for @user do |f| %>
      <ul>
         <li>
            <%= f.label :username %>
            <%= f.text_field :username, autocomplete: :off %>
         </li>
         <li>
           <%= f.label :password %>
           <% f.password_field :password %>
         </li>
      </ul>

      <%= f.submit "Register" %>
    
